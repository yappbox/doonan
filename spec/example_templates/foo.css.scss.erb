.icon1 {
  <%= o("background-image:inline-image('%s')", images.icon_1? && images.icon_1.path) %>
}

.icon2 {
  <%= o("background-image:inline-image('%s')", images.icon_2? && images.icon_2.path) %>
}
<% if image_lists.foo? %>
  <% image_lists.foo.each do |image| %>
    .foo_<%= image.slug %> {
      background-image:inline-image('<%= image.path %>')
    }
  <% end %>
<% end %>
.hello {
  .world {
    <%= o('color', hello_world_color) %>
    <%= o('background-color', my_bg_color) %>
    <%= o('background: %s;', hello_world_color) %>
  }
}
<% my_list.each do |item| %>
  .<%= item.slug %> {
    position: absolute;
    top: <%= item.frame.top %>;
    left: <%= item.frame.left %>;
    width: <%= item.frame.width %>;
    height: <%= item.frame.height %>;
    color: <%= item.color %>;
  }
<% end %>
@import "bay";
@import "shared/bar";
@import "shared/baz";
