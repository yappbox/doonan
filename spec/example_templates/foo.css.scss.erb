.icon1 {
  <% if has_image_icon_1? %>
    background-image:inline-image('<%= image_icon_1.path %>')
  <% end %>
}

.icon2 {
  <% if has_image_icon_2? %>
    background-image:inline-image('<%= image_icon_2.path %>')
  <% end %>
}
<% if has_foo_image_list? %>
  <% foo_images.each do |image| %>
    .foo_<%= image.slug %> {
      background-image:inline-image('<%= image.path %>')
    }
  <% end %>
<% end %>
