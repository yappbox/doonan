.icon1 {
  <% if images.icon_1? %>
    background-image:inline-image('<%= images.icon_1.path %>')
  <% end %>
}

.icon2 {
  <% if images.icon_2? %>
    background-image:inline-image('<%= images.icon_2.path %>')
  <% end %>
}
<% if image_lists.foo? %>
  <% image_lists.foo.each do |image| %>
    .foo_<%= image.slug %> {
      background-image:inline-image('<%= image.path %>')
    }
  <% end %>
<% end %>
.hello {
  .world {
    color: <%= hello_world_color %>;
  }
}
@import "bay";
